{
  "name": "shared-action",
  "version": "1.0.0",
  "description": "Shared actions for better CI Jobs on github actions",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "node ./dist/src/index.js",
