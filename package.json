  "devDependencies": {
    "@angular/cli": "^8.3.21",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/github": "^2.0.0",
    "ansi-colors": "^4.1.1",
